{
  "id": "vite",
  "name": "Vite",
  "category": "build_tool",
  "detect": {
    "npmDependencies": ["vite"],
    "excludedNpmDependencies": ["@shopify/hydrogen", "@builderio/qwik", "solid-start"],
    "configFiles": []
  },
  "dev": {
    "command": "vite",
    "port": 3000,
    "pollingStrategies": [{ "name": "TCP" }]
  },
  "build": {
    "command": "vite build",
    "directory": "dist"
  },
  "logo": {
    "default": "https://framework-info.netlify.app/logos/vite/default.svg",
    "light": "https://framework-info.netlify.app/logos/vite/default.svg",
    "dark": "https://framework-info.netlify.app/logos/vite/default.svg"
  },
  "env": {},
  "plugins": []
}
